{
  "version": 2,
  "builds": [
    {
      "src": "api_handler.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api_handler.py"
    }
  ],
  "env": {
    "DATABASE_URL": "$NEON_DATABASE_URL",
    "SECRET_KEY": "$SECRET_KEY",
    "ALLOWED_ORIGINS": "$ALLOWED_ORIGINS"
  }
}